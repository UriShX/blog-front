{
  "name": "blog-front",
  "version": "1.0.0",
  "description": "vue blog project",
  "author": "David Meir-Levy",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
